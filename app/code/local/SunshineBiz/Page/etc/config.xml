<?xml version="1.0"?>
<config>
    <modules>
        <SunshineBiz_Page>
            <version>1.0.0.0</version>
        </SunshineBiz_Page>
    </modules>
    <global>        
        <blocks>
            <page>
                <rewrite>
                    <html_header>SunshineBiz_Page_Block_Html_Header</html_header>
                    <switch>SunshineBiz_Page_Block_Switch</switch>
                </rewrite>
            </page>
        </blocks>
    </global>
    <frontend>
        <translate>
            <modules>
                <SunshineBiz_Page>
                    <files>
                        <default>SunshineBiz_Page.csv</default>
                    </files>
                </SunshineBiz_Page>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <SunshineBiz_Page>
                    <files>
                        <default>SunshineBiz_Page.csv</default>
                    </files>
                </SunshineBiz_Page>
            </modules>
        </translate>
    </adminhtml>
</config>