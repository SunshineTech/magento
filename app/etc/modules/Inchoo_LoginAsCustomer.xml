<?xml version="1.0"?>
<!--
Magento extension that enables admin users to login as any customer on
the frontend.
-->
<config>
    <modules>
        <Inchoo_LoginAsCustomer>
            <active>true</active>
            <codePool>community</codePool>
        </Inchoo_LoginAsCustomer>
    </modules>
</config>
