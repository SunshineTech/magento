<config>
    <modules>
        <Inchoo_AdminOrderNotifier>
            <active>true</active>
            <codePool>community</codePool>
            <depends>
                <Mage_Sales />
                <Mage_Adminhtml />
            </depends>
        </Inchoo_AdminOrderNotifier>
